<j:jelly xmlns:j="jelly:core">
  <td>
    <j:set var="prop" value="${it.getWorkspaceLogo(job)}"/>
    <j:if test="${prop!=null}">
      <j:if test="${prop.available}">
        <a href="${prop.repositoryUrl}" title="${prop.repositoryUrl}">
            <img src="${resURL}/plugin/gitlab-logo/images/workspace-logo.png" class="icon${iconSize}" />
        </a>
      </j:if>
    </j:if>
  </td>
</j:jelly>
